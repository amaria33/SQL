--Create a store datebase

create table store (ID integer primary key, name text, price integer, popularity integar);
Insert into store VALUES (1, "Pink bra", 10, 99);
Insert into store VALUES (2, "Green bra", 10, 45);
Insert into store VALUES (3, "Red bra", 12, 45);
Insert into store VALUES (4, "zebra bra", 11, 2);
Insert into store VALUES (5, "Purple bra", 10, 65);
Insert into store VALUES (6, "Blue bra", 9, 76);
Insert into store VALUES (7, "Rainbow bra", 8, 86);
Insert into store VALUES (8, "Yellow bra", 10, 86);
Insert into store VALUES (9, "White bra", 10, 97);
Insert into store VALUES (10, "Black bra", 13, 99);
Insert into store VALUES (11, "Turquoise bra", 10, 56);
Insert into store VALUES (13, "Ligiht Pink bra", 14, 89);
Insert into store VALUES (14, "Light Green bra", 14, 45);
Insert into store VALUES (15, "Ribbed bra", 15, 67);

--display the database ordered by price. 
SELECT * FROM store
ORDER BY price desc; 

--what are the most 5 popular items? 
SELECT name, price, popularity
FROM store
order by popularity desc
limit 5; 
